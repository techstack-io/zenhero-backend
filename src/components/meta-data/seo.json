      "repeatable": true,
      "component": "social.metadata-social"
    },
    "preventIndexing": {
      "type": "boolean"
    },
    "structuredData": {
      "type": "json"
    },
    "keywords": {
      "type": "text",
      "regex": "[^,]+"
    }
  }
}
